<resources>
    <!-- Base application theme. -->
    <style name="AppThemeDefault" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <style name="EditTextStyle">
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:background">@color/textColorNew</item>
        <item name="android:gravity">clip_vertical|start</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@color/cursorColor</item>
       <item name="android:paddingStart">8dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingEnd">8dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Large</item>
    </style>

    <style name="backgroundStyle">
        <item name="android:background">@color/newBackGroundColor</item>

    </style>

    <style name="textPresStyle">
        <item name="android:fontFamily">sans-serif-black</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">0dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/textColorNew</item>
    </style>


</resources>
