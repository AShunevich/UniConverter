        android:fontFamily="sans-serif-medium"
        android:gravity="start|center_horizontal"
        android:inputType="none"
        android:lines="1"
        android:paddingStart="8dp"
        android:paddingLeft="8dp"
        android:paddingTop="2dp"
        android:paddingEnd="8dp"
        android:paddingRight="8dp"
        android:paddingBottom="2dp"
        android:selectAllOnFocus="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#253031"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/circleAreaValue"
        app:layout_constraintStart_toEndOf="@+id/circleArea"
        app:layout_constraintTop_toBottomOf="@+id/circleAreaValue" />

    <TextView
        android:id="@+id/diamValue"
        style="@style/angleLeftStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:gravity="start|center_horizontal"
        android:inputType="none"
        android:lines="1"
        android:paddingStart="8dp"
        android:paddingLeft="8dp"
        android:paddingTop="2dp"
        android:paddingEnd="8dp"
        android:paddingRight="8dp"
        android:paddingBottom="2dp"
        android:selectAllOnFocus="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/circleAreaValue"
        app:layout_constraintStart_toStartOf="@+id/radValue"
        app:layout_constraintTop_toBottomOf="@+id/radValue" />

    <TextView
        android:id="@+id/circValue"
        style="@style/angleLeftStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
