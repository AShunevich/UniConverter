        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:fontFamily="sans-serif-medium"
        android:gravity="start|center_horizontal"
        android:inputType="none"
        android:lines="1"
